<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="jntwo_licenseplate_clients">
        <column xsi:type="int" name="entity_id" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
        <column xsi:type="varchar" name="name" nullable="false" comment="Client Name"/>
        <column xsi:type="varchar" name="number" nullable="false" comment="Client Number"/>
        <column xsi:type="varchar" name="taxvat" nullable="false" comment="Client CPF"/>
        <column xsi:type="varchar" name="license_plate" nullable="false" comment="Client License Plate"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="entity_id"/>
        </constraint>
    </table>
</schema>
